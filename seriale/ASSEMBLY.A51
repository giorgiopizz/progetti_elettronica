NAME ASM_CODE

$INCLUDE (C8051F020.INC)

EXTRN 	CODE 	(Init_Device)
PUBLIC START,STACK2,N

CODICE SEGMENT CODE
VETTORE SEGMENT CODE
STACK SEGMENT IDATA
NEXT SEGMENT CODE
RSEG STACK
STACK2: DS 10H

RSEG VETTORE
VETT: DB	'Ciao',00H


RSEG NEXT
N:	MOVC A,@A+DPTR
	RET
RSEG CODICE
START:
	LCALL Init_Device
	MOV DPTR, #VETT
	RET
	
END